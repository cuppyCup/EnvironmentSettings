alias grep='grep --color=auto'
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'

alias ll='ls -l --color=auto'
alias la='ls -a --color=auto'

alias mct='mvn clean test -T 5'
alias mcp='mvn clean package -T 5'
alias mcps='mvn clean package -DskipTests -T 5'
alias mci='mvn clean install -T 5'
alias mcis='mvn clean install -DskipTests -T 5'

alias gp='git push'
alias ga='git add'
alias gc='git commit'
alias gd='git diff'
alias gs='git status'
